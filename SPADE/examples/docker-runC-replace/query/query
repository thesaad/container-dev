%destination_constraint = "inode" == '789939'
$destinations = $base.getVertex(%destination_constraint)
%type_constraint = "type" = 'Artifact'
$Artifact_vertex = $destinations.getVertex(%type_constraint)
$lineage = $base.getLineage($Artifact_vertex, 3, 'b')
$lineage = $lineage.collapseEdge('type')
export > /tmp/runc.dot
dump all $lineage
